{
    "name": "calciumlang",
    "displayName": "calciumlang",
    "description": "Language support for calcium",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.62.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "calcium",
            "aliases": ["Calcium", "calcium"],
            "extensions": [".ca"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "calcium",
            "scopeName": "source.calcium",
            "path": "./syntaxes/calcium.tmLanguage.json"
        }]
    }
}